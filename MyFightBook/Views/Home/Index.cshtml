                    @if (ViewBag.eventtournamentlist != null)
                    {
                        foreach (var eventtournament in ViewBag.eventtournamentlist)
                        {
                            if (eventtournament.ImageArray != null)
                            {
                                int index = rand.Next(eventtournament.ImageArray.Length);
                                var url = "";
                                if (eventtournament.Type == "Event")
                                {
                                    url = "/publicpages/eventId/" + eventtournament.Id;
                                }
                                else
                                {
                                    url = "/publicpages/tournamentid/" + eventtournament.Id;
                                }
                                <a href="@url">
                                    <div class="event-item">
                                        <img src="@eventtournament.ImageArray[index]" />
                                        <h5>@eventtournament.Type</h5>
                                        <div class="event-contant">
                                            <h2>@eventtournament.Name</h2>
                                            <h4>
                                                @eventtournament.StartDate.ToString("dd. MMMM yyyy")
